		"editor.wordBasedSuggestions": "matchingDocuments"
	},
	"cSpell.language": "en,tr-TR,tr",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.words": [
    "supabase",
    "upsert"
  ]
}
